CALL SEND ANKPRM.MAC   '%1(ANKPRM)'    CRLF ASCII
CALL SEND CALLPC.MAC   '%1(CALLPC)'    CRLF ASCII
CALL SEND DSPTBLE.MAC  '%1(DSPTBLE)'   CRLF ASCII
CALL SEND DYNPRM.MAC   '%1(DYNPRM)'    CRLF ASCII
CALL SEND ENDMOD.MAC   '%1(ENDMOD)'    CRLF ASCII
CALL SEND EQUATES.MAC  '%1(EQUATES)'   CRLF ASCII
CALL SEND GET24.MAC    '%1(GET24)'     CRLF ASCII
CALL SEND GETPRM.MAC   '%1(GETPRM)'    CRLF ASCII
CALL SEND HOOKPRE.MAC  '%1(HOOKPRE)'   CRLF ASCII
CALL SEND HOOKPRM.MAC  '%1(HOOKPRM)'   CRLF ASCII
CALL SEND INNERMM.MAC  '%1(INNERMM)'   CRLF ASCII
CALL SEND MODULE.MAC   '%1(MODULE)'    CRLF ASCII
CALL SEND PCSERV.MAC   '%1(PCSERV)'    CRLF ASCII
CALL SEND PERCRC.MAC   '%1(PERCRC)'    CRLF ASCII
CALL SEND PPGNM.MAC    '%1(PPGNM)'     CRLF ASCII
CALL SEND PUT24.MAC    '%1(PUT24)'     CRLF ASCII
CALL SEND SCHDPRM.MAC  '%1(SCHDPRM)'   CRLF ASCII
CALL SEND SRBPRM.MAC   '%1(SRBPRM)'    CRLF ASCII
CALL SEND XMSESA.MAC   '%1(XMSESA)'    CRLF ASCII
CALL SEND XMSXA.MAC    '%1(XMSXA)'     CRLF ASCII
CALL SEND XTOC.MAC     '%1(XTOC)'      CRLF ASCII
